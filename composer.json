{
    "name": "mtn/momoapi-php-sdk",
    "require": {
        "phpunit/phpunit": "^9.6",
        "vlucas/phpdotenv": "^5.5"
    },
    "require-dev": {
        "php": ">=5.4.0",
        "ext-curl": "*",
        "ext-json": "*",
        "dms/phpunit-arraysubset-asserts": "^0.4.0"
    },
    "autoload": {
        "psr-4": {
            "momopsdk\\": "src/",
            "momopsdkTest\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox tests",
        "unit-test": "./vendor/bin/phpunit --testdox --testsuite UnitTest"
    }
}
